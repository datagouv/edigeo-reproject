{
  "name": "edigeo-reproject",
  "version": "1.0.0",
  "description": "Reprojection à la volée des feuilles EDIGÉO",
  "main": "index.js",
  "repository": "git@github.com:etalab/edigeo-reproject.git",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "files": [
    "lib",
    "bin",
    "index.js",
    "zones_cc.json"
  ],
  "bin": "bin/cli.js",
  "scripts": {
    "build-zones": "csvtojson zones_cc.csv > zones_cc.json",
    "test": "nyc ava",
    "lint": "xo",
    "codecov": "codecov -f coverage/coverage-final.json",
    "prepare": "npm run build-zones"
  },
  "dependencies": {
    "decompress": "^4.2.0",
    "epsg-index": "^0.5.0",
    "execa": "^0.8.0",
    "get-stream": "^3.0.0",
    "lodash-es": "^4.17.21",
    "proj4": "^2.4.4",
    "tar-stream": "^1.5.5",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "codecov": "^3.8.2",
    "csvtojson": "^1.1.9",
    "decompress-bzip2": "^4.0.0",
    "nyc": "^15.1.0",
    "xo": "^0.52.3"
  },
  "xo": {
    "space": 2,
    "semicolon": false
  },
  "nyc": {
    "all": true,
    "include": [
      "lib",
      "index.js"
    ],
    "reporter": [
      "html",
      "json",
      "text-summary"
    ]
  },
  "engines": {
    "node": ">= 16"
  }
}
